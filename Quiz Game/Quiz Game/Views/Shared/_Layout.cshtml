<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Quiz Game @ViewBag.Title</title>
    <link href="~/Content/bootstrap.min.css" type="text/css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.min.css" type="text/css" rel="stylesheet" />
    <link href="~/Content/Site.css" type="text/css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.11.1.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/Site.js" type="text/javascript"></script>
</head>
<body onresize="positionMainWindow()">
    <div class="div-game">
        <div class="div-nav div-quiz" goto="/">
            Game
        </div>
        <div class="div-nav div-settings" goto="/Settings">
            Settings
        </div>
        @RenderBody()
    </div>
</body>
</html>